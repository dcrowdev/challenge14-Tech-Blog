<div class="container-fluid">
    {{#each posts as |post|}}
    <div class="card mt-3">
        <a href="/post/{{id}}">
        <div class="card-header">
            {{post_title}}
        </div></a>
        <div class="card-body">
            {{post_body}}
        </div>
    </div>
    {{/each}}
</div>